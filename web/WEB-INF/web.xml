<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>home</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>home</servlet-name>
        <servlet-class>Controller.Common.home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginController</servlet-name>
        <servlet-class>Controller.Common.loginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registerController</servlet-name>
        <servlet-class>Controller.Common.registerController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sendotp</servlet-name>
        <servlet-class>Controller.Common.sendotp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginGoogleController</servlet-name>
        <servlet-class>Controller.Common.LoginGoogleController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logoutController</servlet-name>
        <servlet-class>Controller.Common.logoutController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>changePasswordController</servlet-name>
        <servlet-class>Controller.Common.changePasswordController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BloglistManage</servlet-name>
        <servlet-class>Controller.Admin.BloglistManage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addBlog</servlet-name>
        <servlet-class>Controller.Admin.addBlog</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loadBlogPage</servlet-name>
        <servlet-class>Controller.Customer.loadBlogPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>blogDetail</servlet-name>
        <servlet-class>Controller.Customer.blogDetail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editBlog</servlet-name>
        <servlet-class>Controller.Admin.editBlog</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>exportFile</servlet-name>
        <servlet-class>Controller.Admin.exportFile</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginController</servlet-name>
        <url-pattern>/logincontroller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registerController</servlet-name>
        <url-pattern>/registercontroller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sendotp</servlet-name>
        <url-pattern>/sendotp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginGoogleController</servlet-name>
        <url-pattern>/logingooglecontroller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logoutController</servlet-name>
        <url-pattern>/logoutcontroller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>changePasswordController</servlet-name>
        <url-pattern>/changepassword</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BloglistManage</servlet-name>
        <url-pattern>/bloglistmanage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addBlog</servlet-name>
        <url-pattern>/addblog</url-pattern>
    </servlet-mapping>
